<project><define name="zybuild"/><define name="html5-platform"/>
    <!-- <meta title="ZYGameEngineHaxe" package="com.sample.zygameenginehaxe" version="1.0.0" company="Company Name" /> -->
    <!-- <app main="maplive.core.Maplive" path="Export" file="ZYGameEngineHaxe" /> -->
    <app main="v3api.Main" path="Export" file="ZYGameEngineHaxe"/>

    <!-- 使用V3的用户储存 -->
    <haxedef name="v3apisave"/>
    <!-- <app main="zygame.demo.ShaderDemo" path="Export" file="ZYGameEngineHaxe" /> -->

    <window vsync="true"/>

    <window width="1300" height="900" if="mac"/>

    <template path="templates"/>

    <source path="Source"/>
    <source path="Demo"/>

    <!-- BUILD API DOC -->
    <haxedef name="builddoc"/>

    <!-- <assets path="maplive" rename="maplive"/> -->

    <haxelib name="openfl"/>
    <haxelib name="hscript"/>
    <haxelib name="spine-hx" version="3.8.2" if="spine380"/>
    <haxelib name="spine-hx" version="3.6.0" unless="spine380"/>
    <haxelib name="tweenxcore"/>
    <haxelib name="castle"/>
    <haxelib name="differ"/>
    <haxelib name="pomelo-haxe"/>
    <haxelib name="json2object"/>
    <haxelib name="zygameui-3d"/>
    <!-- <haxelib name="KengSDK"/> -->

    <window allow-high-dpi="true"/>
    <haxeflag name="-xml" value="bin/doc.xml"/>

    <haxedef name="gl_stats"/>

    <!-- zygameui框架标示 -->
    <haxedef name="zygame"/>
    <haxedef name="maplive"/>
    <haxedef name="debug"/>

    <!-- 默认平滑 -->
    <haxedef name="smoothing"/>

    <!-- <haxelib name="qqplaycore"/> -->
    <set name="VIEW_MODE" value="1"/>

    <!-- <assets path="1px.png" rename="1px.png" /> -->

    <!-- 编译梦工厂时，需要对mgc做兼容处理 -->
    <!-- <haxedef name="mgc" if="mgc" /> -->
    <!-- <assets path="Assets" rename="assets" embed="false" /> -->
</project>